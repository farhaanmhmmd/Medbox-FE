\r\n                  -\r\n                </Button>\r\n              )}\r\n\r\n              <Input\r\n                type={'text'}\r\n                width={'70px'}\r\n                value={quantity}\r\n                onChange={onHandleQuantityChange}\r\n              ></Input>\r\n              <Button\r\n                colorScheme={'linkedin'}\r\n                variant={'ghost'}\r\n                onClick={() => {\r\n                  setQuantity(quantity + 1);\r\n                }}\r\n              >\r\n                +\r\n              </Button>\r\n            </HStack>\r\n          </ModalBody>\r\n\r\n          <Box rounded={10} border=\"1px\" borderColor=\"gray.100\" py={2}>\r\n            <VStack>\r\n              {tempFormulaMap()}\r\n              {tempFormula.length ? (\r\n                <Button\r\n                  height={'20px'}\r\n                  width={'110px'}\r\n                  variant={'ghost'}\r\n                  fontSize={'small'}\r\n                  fontWeight={'normal'}\r\n                  colorScheme={'red'}\r\n                  isLoading={loading}\r\n                  onClick={() => {\r\n                    setTempFormula([]);\r\n                  }}\r\n                >\r\n                  Hapus Semua\r\n                </Button>\r\n              ) : (\r\n                <></>\r\n              )}\r\n            </VStack>\r\n          </Box>\r\n\r\n          <ModalFooter>\r\n            <Button\r\n              variant=\"ghost\"\r\n              colorScheme=\"red\"\r\n              mr={3}\r\n              onClick={() => {\r\n                setAddFormulaButton(false), setTempFormula([]), setQuantity(0);\r\n                setDeleted(0), setName(''), setAmount(0);\r\n              }}\r\n            >\r\n              Batal\r\n            </Button>\r\n            {quantity == 0 || option == '' ? (\r\n              <Button\r\n                mr={2}\r\n                isDisabled\r\n                colorScheme=\"linkedin\"\r\n     